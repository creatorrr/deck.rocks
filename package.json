  },
  "homepage": "https://github.com/creatorrr/deck.rocks#readme",
  "devDependencies": {
    "@types/bull": "^3.15.8",
    "@types/classnames": "^2.3.1",
    "@types/dotenv": "^8.2.0",
    "@types/glob": "^7.2.0",
    "@types/ioredis": "^4.28.10",
    "@types/koa": "^2.13.4",
    "@types/koa__router": "^8.0.11",
    "@types/koa-ratelimit": "^4.2.4",
    "@types/koa-static-cache": "^5.1.1",
    "@types/lodash": "^4.14.182",
